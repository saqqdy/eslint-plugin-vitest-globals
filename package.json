{
	"name": "eslint-plugin-vitest-globals",
	"description": "A extends of vitest globals for eslint",
	"version": "1.2.0",
	"packageManager": "pnpm@7.7.0",
	"main": "index.js",
	"scripts": {
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"dist": "pnpm run prettier",
		"prettier": "prettier --write \"**/*.{js,ts,sh,json,md}\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"eslint-config-sets": "^1.8.3"
	},
	"devDependencies": {
		"@babel/core": "^7.18.13",
		"@eslint-sets/eslint-config-simple-ts": "^3.0.4",
		"@types/node": "^18.7.13",
		"eslint": "^8.23.0",
		"prettier": "^2.7.1",
		"prettier-config-common": "^1.2.1",
		"tsnd": "^1.1.0",
		"typescript": "^4.8.2"
	},
	"keywords": [
		"vitest",
		"vitest env",
		"vitest globals",
		"eslint-plugin-vitest-globals",
		"eslint-plugin",
		"eslint",
		"eslint-extends"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "http://github.com/saqqdy/eslint-plugin-vitest-globals.git"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"tslib",
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	}
}
